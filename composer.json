{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2",
        "ext-curl": "^7.2",
        "ext-iconv": "*",
        "ext-json": "^1.6",
        "guzzlehttp/guzzle": "^7.0",
        "nelmio/api-doc-bundle": "^3.3",
        "nelmio/cors-bundle": "^1.5",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "*",
        "symfony/orm-pack": "*",
        "symfony/security-bundle": "*",
        "symfony/serializer": "*",
        "symfony/twig-bundle": "*",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "symfony/maker-bundle": "*",
        "symfony/profiler-pack": "*",
        "symfony/var-dumper": "4.2.*",
        "symfony/web-server-bundle": "4.2.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    }
}
